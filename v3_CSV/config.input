# configuration file for bin/build_connections_graph.py
# Ben Payne <ben.is.located@gmail.com>

make_texPDF_for_set_boolean: False
make_graphviz_boolean:       False # the big graph
make_graphml_boolean:        True
make_networkx_boolean:       True

redraw_feed_pictures_boolean:           False
redraw_statement_pictures_boolean:      False
redraw_inference_rule_pictures_boolean: False

# png or svg
file_extension_string: 'png'   

# extension will be appended to picture path
expr_latex_to_pictures_path:    'lib/images_expression_'
feed_latex_to_pictures_path:    'lib/images_feed_'
infrule_latex_to_pictures_path: 'lib/images_infrule_'

expr_pictures_json_path:    'http://allofphysicsgraph.github.io/proofofconcept/v3_CSV/lib/images_expression_'
feed_pictures_json_path:    'http://allofphysicsgraph.github.io/proofofconcept/v3_CSV/lib/images_feed_'
infrule_pictures_json_path: 'http://allofphysicsgraph.github.io/proofofconcept/v3_CSV/lib/images_infrule_'

# output path requires trailing /
output_path:           'output/'

# these are the CSV databases
connectionsDB_path:    'databases/connections_database.csv'
expressionsDB_path:    'databases/expressions_database.csv'
feedDB_path:           'databases/feed_database.csv'
infruleDB_path:        'inference_rules_database.csv'
